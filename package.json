{
	"name": "quizdd-frontend",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"build:dev": "tsc && vite build --mode development",
		"build:optimized": "tsc && vite build --mode production --minify",
		"preview": "vite preview",
		"lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"type-check": "tsc --noEmit",
		"test": "vitest --run",
		"test:run": "vitest --run",
		"test:watch": "vitest",
		"test:coverage": "vitest --run --coverage",
		"tauri:dev": "cargo tauri dev",
		"tauri:build": "cargo tauri build",
		"tauri:build:debug": "cargo tauri build --debug",
		"package": "node scripts/build-and-package.js",
		"package:windows": "node scripts/build-and-package.js --platform windows",
		"package:macos": "node scripts/build-and-package.js --platform macos",
		"package:linux": "node scripts/build-and-package.js --platform linux",
		"optimize:memory": "node scripts/analyze-memory.js",
		"optimize:bundle": "node scripts/analyze-bundle.js",
		"seed:content": "cargo run --bin seed_database",
		"seed:expanded": "cargo run --bin seed_database -- --expanded"
	},
	"dependencies": {
		"@tauri-apps/api": "^2.8.0",
		"lottie-react": "^2.4.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@types/node": "^24.7.2",
		"@types/react": "^18.2.43",
		"@types/react-dom": "^18.2.17",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"@vitejs/plugin-react": "^4.2.1",
		"eslint": "^8.55.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"typescript": "^5.2.2",
		"vite": "^5.0.8",
		"vitest": "^1.0.0",
		"@testing-library/react": "^14.0.0",
		"@testing-library/jest-dom": "^6.0.0",
		"@testing-library/user-event": "^14.0.0",
		"@vitest/coverage-v8": "^1.0.0",
		"jsdom": "^23.0.0"
	}
}